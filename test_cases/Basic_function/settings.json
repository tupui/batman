{
    "space": {
        "corners": [
            [-4.0],
            [4.0]
        ],
        "sampling": {
            "init_size": 4,
            "method": "halton"
        },
        "resampling":{
            "delta_space": 0.08,
            "resamp_size": 0,
            "method": "discrepancy",
            "hybrid": [["sigma", 4], ["loo_sobol", 2]],
            "q2_criteria": 0.9
        }
    },
    "snapshot": {
        "max_workers": 5,
        "plabels": ["x1"],
        "flabels": ["F"],
        "provider": {
            "type": "file",
            "command": "bash script.sh",
            "context_directory": "data",
            "coupling_directory": "batman-coupling",
            "timeout": 10,
            "clean": false,
            "restart": "False"
        },
        "io": {
            "point_filename": "point.json",
            "data_filename": "point.npz",
            "data_format": "numpy"
        }
    },
    "surrogate": {
        "predictions": [2.2],
        "method": "rbf"
    },
    "uq": {
        "sample": 1000,
        "pdf": ["Normal(0., 4.)"],
        "type": "aggregated",
        "method": "sobol"
    }
}
