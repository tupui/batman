{
    "space": {
        "corners": [
            [20.0, 1000.0],
            [40.0, 3000.0]
        ],
        "sampling": {
            "init_size": 121,
            "method": "halton",
            "distributions": ["Uniform(20., 40.)", "BetaMuSigma(2000, 500, 1000, 3000).getDistribution()"]
        },
        "resampling":{
            "delta_space": 0.08,
            "resamp_size": 0,
            "method": "sigma",
            "hybrid": [["sigma", 4], ["loo_sobol", 2]],
            "q2_criteria": 0.9
        }
    },
    "pod": {
        "dim_max": 100,
        "tolerance": 0.99,
        "type": "static"
    },
    "snapshot": {
        "max_workers": 5,
        "io": {
            "shapes": {
                "0": [
                    [51]
                ]
            },
            "format": "fmt_tp_fortran",
            "variables": ["F"],
            "point_filename": "header.py",
            "filenames": {
                "0": ["function.dat"]
            },
            "template_directory": null,
            "parameter_names": ["x1", "x2"]
        },
        "provider": {
            "command": "bash",
            "timeout": 10,
            "context": "data",
            "script": "data/script.sh",
            "clean": false,
            "private-directory": "batman-data",
            "data-directory": "cfd-output-data",
            "restart": "False"
        }
    },
    "surrogate": {
        "predictions": [[30, 2000]],
        "method": "pc",
        "strategy": "Quad",
        "degree": 10
    },
    "uq": {
        "sample": 10000,
        "pdf": ["Uniform(20., 40.)", "BetaMuSigma(2000, 500, 1000, 3000).getDistribution()"],
        "type": "aggregated",
        "method": "sobol"
    }
}
