{
    "space": {
        "corners": [
            [-3.1415, -3.1415, -3.1415],
            [3.1415, 3.1415, 3.1415]
        ],
        "sampling": {
            "init_size": 4,
            "method": "halton"
        },
        "resampling":{
            "delta_space": 0.08,
            "resamp_size": 0,
            "method": "sigma",
            "hybrid": [["sigma", 4], ["loo_sobol", 2]],
            "q2_criteria": 0.9
        }
    },
    "pod": {
        "dim_max": 100,
        "tolerance": 0.99,
        "server": null,
        "type": "static"
    },
    "snapshot": {
        "max_workers": 10,
        "io": {
            "shapes": {
                "0": [
                    [1]
                ]
            },
            "format": "numpy",
            "variables": ["F"],
            "point_filename": "header.py",
            "filenames": {
                "0": ["function.npz"]
            },
            "template_directory": "output/snapshots/0/jpod-data",
            "parameter_names": ["x1", "x2", "x3"]
        },
        "provider": {
            "command": "bash",
            "timeout": 10,
            "context": "data",
            "script": "data/script.sh",
            "clean": false,
            "private-directory": "jpod-data",
            "data-directory": "cfd-output-data",
            "restart": "False"
        }
    },
    "surrogate": {
        "predictions": [[1, 1, 1]],
        "method": "kriging"
    },
    "uq": {
        "sample": 2000,
        "test": "Ishigami",
        "pdf": ["Uniform(-3.1415, 3.1415)", "Uniform(-3.1415, 3.1415)", "Uniform(-3.1415, 3.1415)"],
        "type": "aggregated",
        "method": "sobol"
    }
}
